 <body>
  <!-- Navigation ends here -->

<div class="personalinfocontrollerindex container">
  <div class="row container">
    <div class="col-lg-3 col-md-3 column1">
      <div class="column11">
        <li><a href="personalinfos/edit"><h2>Edit</h2></a></li>
      </div>
    </div>
    <div class="col-lg-1 col-md-1"></div>
    <div class="col-lg-6 col-md-6">
      <% if Personalinfo.where(:user_id => session[:user_id]).present? %>
        <% @check = Personalinfo.find_by_user_id(session[:user_id]) %>
        <h2><%= @check.user_id %></h2>
        <h2><%= @check.name %></h2>
        <h2><%= @check.phoneno %></h2>
        <h2><%= @check.emailid %></h2>
        <% puts "Yeah yeah it is coming" %>
      <% else %>
        <h1>Nothing Exist Press Edit </h1>
      <% end %>
    </div>
  </div>    
</div>

